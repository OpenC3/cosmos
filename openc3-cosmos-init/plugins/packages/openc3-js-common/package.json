  "dependencies": {
    "@anycable/web": "1.1.0",
    "@astrouxds/astro-web-components": "7.27.0",
    "axios": "1.13.1",
    "binary-search": "1.3.6",
    "date-fns": "4.1.0",
    "date-fns-tz": "3.2.0",
    "lodash": "4.17.21",
    "lossless-json": "4.3.0",
    "sass": "1.93.3",
    "sortablejs": "1.15.6",
    "sprintf-js": "1.1.3",
